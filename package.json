{
  "name": "the-react-movies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --config ./webpack/webpack.config.js --open",
    "build": "webpack --mode production --config ./webpack/webpack.config.js",
    "dev": "webpack --mode development --config ./webpack/webpack.config.js",
    "build-analyze": "cross-env WEBPACK_ANALYZER=1 build",
    "build:dll": "webpack --config ./webpack/webpack.dll.js",
    "postinstall": "npm run build:dll",
    "test": "jest",
    "eject": "react-scripts eject",
    "precommit": "cross-env NODE_ENV=production lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --config .prettierrc",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
